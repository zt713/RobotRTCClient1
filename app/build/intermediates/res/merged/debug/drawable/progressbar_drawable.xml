<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#ffcccdce" android:endColor="#ffcccdce" android:angle="270.0" android:centerY="2.0" android:centerColor="#ffcccdce" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff0d7dfb" android:endColor="#ff0d7dfb" android:angle="270.0" android:centerY="2.0" android:centerColor="#ff0d7dfb" />
            </shape>
        </clip>
    </item>
</layer-list>